        "name": "Death Counter",
        "author": "Blasstah",
        "desc": "Adds a death counter to the Player List",
        "apply": [
            "scoreboard objectives add deathCounter deathCount \"Deaths\"",
            "scoreboard objectives setdisplay list deathCounter"
        ],
        "revert": [
            "scoreboard objectives remove deathCounter"
        ]
    },
